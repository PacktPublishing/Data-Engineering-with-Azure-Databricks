{
  "datasets": [
    {
      "name": "sales_daily_metrics",
      "displayName": "Daily Sales Metrics",
      "queryLines": [
        "SELECT * FROM sales_daily_metrics ORDER BY date"
      ],
      "catalog": "analytics_dev",
      "schema": "sales_gold"
    },
    {
      "name": "category_performance",
      "displayName": "Category Performance",
      "queryLines": [
        "SELECT * FROM category_performance"
      ],
      "catalog": "analytics_dev",
      "schema": "sales_gold"
    }
  ],
  "pages": [
    {
      "name": "sales_overview",
      "displayName": "Sales Overview",
      "layout": [
        {
          "widget": {
            "name": "total_revenue_kpi",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "sales_daily_metrics",
                  "fields": [
                    {
                      "name": "monthly(date)",
                      "expression": "DATE_TRUNC(\"MONTH\", `date`)"
                    },
                    {
                      "name": "sum(total_revenue)",
                      "expression": "SUM(`total_revenue`)"
                    }
                  ],
                  "disaggregated": false,
                  "orders": [
                    {
                      "direction": "DESC",
                      "expression": "DATE_TRUNC(\"MONTH\", `date`)"
                    }
                  ]
                }
              }
            ],
            "spec": {
              "version": 2,
              "frame": {
                "showTitle": true,
                "showDescription": false,
                "title": "Revenue Month-to-date"
              },
              "widgetType": "counter",
              "encodings": {
                "value": {
                  "fieldName": "sum(total_revenue)",
                  "displayName": "Total Revenue"
                },
                "period": {
                  "fieldName": "monthly(date)"
                }
              }
            }
          },
          "position": {
            "x": 0,
            "y": 0,
            "width": 2,
            "height": 2
          }
        },
        {
          "widget": {
            "name": "total_orders_kpi",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "sales_daily_metrics",
                  "fields": [
                    {
                      "name": "weekly(date)",
                      "expression": "DATE_TRUNC(\"WEEK\", `date`)"
                    },
                    {
                      "name": "sum(total_orders)",
                      "expression": "SUM(`total_orders`)"
                    }
                  ],
                  "disaggregated": false,
                  "orders": [
                    {
                      "direction": "DESC",
                      "expression": "DATE_TRUNC(\"WEEK\", `date`)"
                    }
                  ]
                }
              }
            ],
            "spec": {
              "version": 2,
              "frame": {
                "showTitle": true,
                "title": "Orders This Week"
              },
              "widgetType": "counter",
              "encodings": {
                "value": {
                  "fieldName": "sum(total_orders)",
                  "displayName": "Total Orders"
                },
                "period": {
                  "fieldName": "weekly(date)"
                }
              }
            }
          },
          "position": {
            "x": 2,
            "y": 0,
            "width": 2,
            "height": 2
          }
        },
        {
          "widget": {
            "name": "avg_order_value_kpi",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "sales_daily_metrics",
                  "fields": [
                    {
                      "name": "monthly(date)",
                      "expression": "DATE_TRUNC(\"MONTH\", `date`)"
                    },
                    {
                      "name": "avg(average_order_value)",
                      "expression": "AVG(`average_order_value`)"
                    }
                  ],
                  "disaggregated": false,
                  "orders": [
                    {
                      "direction": "DESC",
                      "expression": "DATE_TRUNC(\"MONTH\", `date`)"
                    }
                  ]
                }
              }
            ],
            "spec": {
              "version": 2,
              "frame": {
                "showTitle": true,
                "title": "Avg Order Value This Month"
              },
              "widgetType": "counter",
              "encodings": {
                "value": {
                  "fieldName": "avg(average_order_value)",
                  "displayName": "Avg Order Value"
                },
                "period": {
                  "fieldName": "monthly(date)"
                }
              }
            }
          },
          "position": {
            "x": 4,
            "y": 0,
            "width": 2,
            "height": 2
          }
        },
        {
          "widget": {
            "name": "daily_revenue_trend",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "sales_daily_metrics",
                  "fields": [
                    {
                      "name": "monthly(date)",
                      "expression": "DATE_TRUNC(\"MONTH\", `date`)"
                    },
                    {
                      "name": "sum(total_orders)",
                      "expression": "SUM(`total_orders`)"
                    }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "frame": {
                "showTitle": true,
                "title": "Monthly Sales Trend"
              },
              "widgetType": "line",
              "encodings": {
                "x": {
                  "fieldName": "monthly(date)",
                  "scale": {
                    "type": "temporal"
                  }
                },
                "y": {
                  "fieldName": "sum(total_orders)",
                  "scale": {
                    "type": "quantitative"
                  }
                },
                "label": {
                  "show": false
                }
              }
            }
          },
          "position": {
            "x": 0,
            "y": 2,
            "width": 6,
            "height": 4
          }
        },
        {
          "widget": {
            "name": "daily_orders_trend",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "sales_daily_metrics",
                  "fields": [
                    {
                      "name": "date",
                      "expression": "`date`"
                    },
                    {
                      "name": "total_orders",
                      "expression": "`total_orders`"
                    }
                  ],
                  "disaggregated": true
                }
              }
            ],
            "spec": {
              "version": 2,
              "widgetType": "line",
              "encodings": {
                "x": {
                  "fieldName": "date",
                  "scale": {
                    "type": "temporal"
                  },
                  "displayName": "Date"
                },
                "y": {
                  "fieldName": "total_orders",
                  "scale": {
                    "type": "quantitative"
                  },
                  "displayName": "Orders"
                }
              }
            }
          },
          "position": {
            "x": 0,
            "y": 6,
            "width": 3,
            "height": 4
          }
        },
        {
          "widget": {
            "name": "daily_aov_trend",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "sales_daily_metrics",
                  "fields": [
                    {
                      "name": "date",
                      "expression": "`date`"
                    },
                    {
                      "name": "average_order_value",
                      "expression": "`average_order_value`"
                    }
                  ],
                  "disaggregated": true
                }
              }
            ],
            "spec": {
              "version": 2,
              "widgetType": "line",
              "encodings": {
                "x": {
                  "fieldName": "date",
                  "scale": {
                    "type": "temporal"
                  },
                  "displayName": "Date"
                },
                "y": {
                  "fieldName": "average_order_value",
                  "scale": {
                    "type": "quantitative"
                  },
                  "displayName": "AOV"
                }
              }
            }
          },
          "position": {
            "x": 3,
            "y": 6,
            "width": 3,
            "height": 4
          }
        },
        {
          "widget": {
            "name": "category_revenue_bar",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "category_performance",
                  "fields": [
                    {
                      "name": "category",
                      "expression": "`category`"
                    },
                    {
                      "name": "total_revenue",
                      "expression": "`total_revenue`"
                    }
                  ],
                  "disaggregated": true
                }
              }
            ],
            "spec": {
              "version": 2,
              "widgetType": "bar",
              "encodings": {
                "x": {
                  "fieldName": "total_revenue",
                  "scale": {
                    "type": "quantitative"
                  },
                  "displayName": "Revenue"
                },
                "y": {
                  "fieldName": "category",
                  "scale": {
                    "type": "categorical"
                  },
                  "displayName": "Category"
                }
              }
            }
          },
          "position": {
            "x": 0,
            "y": 10,
            "width": 3,
            "height": 4
          }
        },
        {
          "widget": {
            "name": "category_units_bar",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "category_performance",
                  "fields": [
                    {
                      "name": "category",
                      "expression": "`category`"
                    },
                    {
                      "name": "total_units_sold",
                      "expression": "`total_units_sold`"
                    }
                  ],
                  "disaggregated": true
                }
              }
            ],
            "spec": {
              "version": 2,
              "widgetType": "bar",
              "encodings": {
                "x": {
                  "fieldName": "total_units_sold",
                  "scale": {
                    "type": "quantitative"
                  },
                  "displayName": "Units Sold"
                },
                "y": {
                  "fieldName": "category",
                  "scale": {
                    "type": "categorical"
                  },
                  "displayName": "Category"
                }
              }
            }
          },
          "position": {
            "x": 3,
            "y": 10,
            "width": 3,
            "height": 4
          }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS"
    }
  ]
}
